{
  "name": "hello-next-app",
  "description": "React Web App Project Template with Next.js, Firebase, Theme UI, MDX and more.",
  "version": "0.0.1",
  "scripts": {
    "dev": "NODE_ENV=development next dev",
    "build": "NODE_ENV=production next build",
    "start": "NODE_ENV=production next start",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "format-watch": "npm run format && onchange \"**/*.{js,jsx,json,md}\" -- prettier --write {{changed}}",
    "test": "cypress run",
    "cypress": "cypress open",
    "deploy": "npm run test && git push origin main && vercel --prod"
  },
  "dependencies": {
    "@mdx-js/loader": "^1.6.22",
    "@mdx-js/runtime": "^1.6.22",
    "@next/mdx": "^10.0.4",
    "@stripe/react-stripe-js": "^1.1.2",
    "@stripe/stripe-js": "^1.11.0",
    "@theme-ui/color": "^0.3.5",
    "cookie-session": "1.4.0",
    "dotenv": "8.2.0",
    "firebase": "^7.24.0",
    "firebase-admin": "^8.13.0",
    "gray-matter": "^4.0.2",
    "lodash": "^4.17.20",
    "next": "^10.0.4",
    "next-iron-session": "^4.1.10",
    "nookies": "^2.5.0",
    "prop-types": "15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-ga": "^3.3.0",
    "react-social-icons": "^4.1.0",
    "stripe": "^8.129.0",
    "swr": "^0.3.11",
    "theme-ui": "^0.3.5"
  },
  "devDependencies": {
    "cypress": "^6.2.0",
    "onchange": "^7.1.0",
    "prettier": "^2.2.1"
  },
  "license": "SEE LICENSE IN EULA.txt",
  "repository": "https://github.com/johnpolacek/hello-next-app"
}
